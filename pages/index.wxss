page {
  background-color: #e9ddc3;
}

.fb-headtxt {
  background: #f1e9d4 url('data:image/jpg;base64,') no-repeat;
  background-size: 100% auto;
  margin-bottom: 20rpx;
  min-height: 200rpx;
}

.fb-headtxt .txt {
  padding: 20rpx 20rpx 20rpx 160rpx;
  color: #bb8962;
}

.fb-input-group {
  padding: 20rpx 0;
  background-color: #f2e9d4;
}

.input-row {
  display: flex;
  align-items: center;
  width: 90%;
  margin: 0 auto;
  margin-bottom: 16rpx;
}

.fb-input {
  background-image: url("data:image/png;base64,");
  background-size: 100% 80rpx;
  height: 80rpx;
  font-size: 32rpx;
}

.fb-input .lable-text {
  display: flex;
  align-items: center;
  height: 100%;
  color: #BA8961;
  width: 20%;
  padding-left: 28rpx;
}

.fb-input input {
  outline: none;
  height: 100%;
  background: 0 0;
  border: 0;
  width: 80%;
}

.fb-radio radio-group {
  height: 100%;
  width: 80%;
  color: #bb8962;
  display: flex;
  align-items: center;
}

.fb-input .fb-radio input {
  height: 40rpx;
  width: 40rpx;
}

.button-row {
  text-align: center;
  margin-top: 40rpx;
  margin-bottom: 40rpx;
}

.fb-input-but {
  outline: none;
  border-radius: 12rpx;
  color: #fff;
  border: none;
  background-color: #94181a;
  /* padding: 20rpx 120rpx; */
}

.calculation-tips {
  font-size: 24rpx;
  width: 90%;
  margin: 0 auto;
  background-color: #fff2f1;
  border: solid 2rpx #ffae9c;
  color: #6c6a65;
}

.calculation-tips view {
  margin: 0;
  padding: 4rpx;
}

.fb-container {
  margin: 20rpx;
}

.fb-content {
  background-color: #f2e9d4;
  padding: 20rpx;
}

.fb-content image {
  width: 100%;
  height: 104rpx;
}

.query-content {
  font-size: 36rpx;
  margin-top: 20rpx;
  border-top: 2rpx solid #e4d3c1;
  color: #bb8962;
}

.fb-clearfix {
  padding: 12rpx 0;
  border-bottom: 2rpx solid #e4d3c1;
}

.txt-col-red {
  color: #94181a;
}

.font20 {
  font-size: 40rpx;
}

.label-col-zs {
  margin-top: 12rpx;
  background-color: #bb8a62;
  color: #f0e9d6;
  text-align: center;
  padding: 4rpx 20rpx;
  border-radius: 0 0 6rpx 6rpx;
  margin-bottom: 20rpx;
  position: relative;
  display: inline-block;
}

.label-col-zs i {
  background-color: #bb8a62;
  position: absolute;
  bottom: -6rpx;
  right: 48%;
  width: 10rpx;
  height: 10rpx;
  transform: rotate(45deg);
  -webkit-transform: rotate(45deg);
}

.not-found {
  text-align: center;
}

.not-found image {
  width: 256rpx;
  height: 256rpx;
  margin-top: 20rpx;
}

.not-found div {
  color: #94181a;
  margin-top: 20rpx;
}

.cylinder-content {
  text-align: center;
}

.cylinder-content image {
  width: 144rpx;
  height: 144rpx;
}

.cylinder-content button {
  outline: none;
  border-radius: 12rpx;
  color: #fff;
  border: none;
  background-color: #94181a;
  /* padding: 20rpx 120rpx; */
  margin: 20rpx 0;
}
